<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mldn.dibmp.dao.IMemberDAO">
	<select id="findById" parameterType="String" resultType="Member">
		SELECT mid,lid,did,name,salary,phone,password,photo,note,hiredate,inmid,locked FROM member WHERE mid=#{mid}
	</select>
	<select id="findAllLevel" resultType="Level">
		SELECT lid,title,losal,hisal FROM level 
	</select>
	<select id="findAllDept" resultType="Dept">
		SELECT did,dname,mid,maxnum,currnum FROM dept
	</select>
</mapper>