<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mldn.dibmp.dao.ICustomerDAO">
	<insert id="doCreate" parameterType="Customer">
		INSERT INTO
		customer(name,phone,pid,cid,address,inDate,connum,ciid,csid,note,recorder)
		VALUES
		(#{name},#{phone},#{pid},#{cid},#{address},#{inDate},#{connum},#{ciid},#{csid},#{note},#{recorder})
	</insert>
	
</mapper>